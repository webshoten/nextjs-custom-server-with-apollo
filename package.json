{
  "name": "customserver",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.build.json server/index.ts",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.build.json",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production ts-node --project tsconfig.build.json dist/index.js",
    "drizzle:generate": "drizzle-kit generate:pg --config=./drizzle.config.ts",
    "drizzle:push": "drizzle-kit push:pg --config=./drizzle.config.ts",
    "drizzle:introspect": "drizzle-kit introspect:pg --config=./drizzle.config.ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@apollo/experimental-nextjs-app-support": "^0.10.1",
    "@apollo/server": "^4.10.4",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@neondatabase/serverless": "^0.9.3",
    "@react-oauth/google": "^0.12.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.30.10",
    "express": "^4.18.3",
    "google-auth-library": "^9.10.0",
    "graphql": "^16.8.1",
    "next": "^14.2.3",
    "postgres": "^3.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.4",
    "@graphql-codegen/introspection": "4.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/graphql": "^14.5.0",
    "@types/node": "^20.11.26",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.18",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
